{% extends 'IEPCWebsiteBundle::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets output='css/iepcjaliscoadmin.css' filter='?yui_css'
        '@IEPCAdminBundle/Resources/less/iepcjaliscoadmin.less'
    %}
    <link rel="stylesheet" href="{{ asset_url }}">
    {% endstylesheets %}
{% endblock stylesheets %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts output='js/iepcjaliscoadmin.js' filter='?yui_js'
        'assets/vendor/angular/angular.js'
        'assets/vendor/tinymce/jquery.tinymce.js'
        '@IEPCAdminBundle/Resources/javascript/admincontent.js'
    %}
    <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock javascripts %}

{% block layout '' %}